<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ClosedDuplicateRelateRequest</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
(ISPICKVAL(Status__c, &quot;Close: Duplicate&quot;)),
(ISBLANK(Related_Request__c))
)</errorConditionFormula>
    <errorDisplayField>Related_Request__c</errorDisplayField>
    <errorMessage>Must Enter Related Request Value</errorMessage>
</ValidationRule>
