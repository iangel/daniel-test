<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_End_Date</fullName>
    <active>true</active>
    <errorConditionFormula>End_Date__c &lt;  Start_Date__c</errorConditionFormula>
